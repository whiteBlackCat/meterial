@keyframes circle{
	0%{transform: rotate(0deg)}
	100%{ transform: rotate(360deg)}
}
@keyframes cnloading{
	0%{ transform: translateY(0rpx)}
	50%{ transform: translateY(20rpx)}
	100%{ transform: translateY(0rpx)}
}
.fc_gray{color: #949597}
#playingpage{overflow: hidden;height: 100%;position: relative;color: #fff}

.blurbg{
    filter: blur(30px) brightness(.8) ;
	background-size: cover;
	background-position: center center;
	background-repeat: no-repeat;
	position: absolute;
	width: 100%;
	height: 100%;
	z-index: -1;
	left: 0;
	top: 0;
	transform: scale(1.2);
	background-color: #aaa
}
 
.cntloading{padding: 50rpx 0;text-align: center;color: #888;font-size: 26rpx;}
.cntloading view{display: inline-block;vertical-align: baseline;height: 30rpx;overflow: hidden;margin-left: 10rpx;}
.cntloading text{width: 5rpx;height: 30rpx;background-color: #d33a31;position: relative;display: inline-block;margin-right: 6rpx;
border-radius: 3rpx;animation: cnloading .9s linear infinite; transform-origin: center bottom;}
.cntloading .cl2{ animation-delay: -.6s}
.cntloading .cl3{animation-delay: -.3s}

 
#playing-status{display: flex;color:#fff;margin: 20rpx}
#playing-status progress{ width:520rpx;margin: 0 auto;color: #000;position: relative;border-radius: 4rpx;}
#playing-status progress text{position: absolute;display: block;height:8rpx;width: 8rpx;overflow: hidden;background-color: #d33a31;
border:12rpx solid #fff;border-radius: 50%;top:50%; transform: translate3d(-50%,-50%,0)}
#playing-status .time{width:80rpx ;text-align:center;display: block;font-size: 24rpx;}

.tab{display: flex;border-bottom: 1rpx solid #dfdfdf; background: #f8f8f8;height: 80rpx;line-height: 80rpx;}
.tab .tab-item{text-align: center; flex:1; word-break: keep-all}
.tab .tab-item text{display: inline-block;padding: 0 .3em;}
.tab .tab-item .tbi-des{display: inline;color: #999; border-bottom: none;font-size: 24rpx;line-height: 1}
.tbi-cur text{color: #d33a31;border-bottom: 4rpx solid #d33a31; line-height: 77rpx;height: 77rpx; margin-bottom: -1rpx}



.rec_list{display: flex;padding: 20rpx 0 0 20rpx;}
.rec_avatar,.rec_avatar image{width: 60rpx;height: 60rpx;border-radius: 50%;}
.rec_main{border-bottom: 1rpx solid #e1e2e3;margin-left: 20rpx;width:650rpx;font-size: 28rpx;padding: 0 20rpx 15rpx 0;line-height: 48rpx;}

.rm_header{font-size: 24rpx;color: #999;line-height:1.8;padding-bottom: 10rpx;}
.rm_header view{float: right;line-height: 40rpx;}
.fmh_time{display: block;font-size: 18rpx;}
.rm_header .fmh_name{font-size: 28rpx;color: #333;display:block}
.rm_header view image{width:40rpx;height: 40rpx;margin-left: 10rpx;vertical-align: sub}
.rmc_reply{margin:10rpx 0 15rpx;padding: 20rpx;color: #888;background-color: #fdfefe;border: 1rpx solid #e4e4e5}
.replyname{color: #537fb0}


.playinghidden{z-index:-1;opacity:0;pointer-events: none;}
 
#lrclist{position: absolute;left: 30rpx;top:30rpx;right: 30rpx;bottom:260rpx;text-align: center;overflow: hidden;color: #fff;z-index: 3;}

#lrcwrap{top:50%;position: absolute; font-size: 30rpx;width:100%; transition: all ease 1s;}
#lrcwrap .lrcur{opacity: 1;font-size: 31rpx; transition: all ease 1s;}
#lrcwrap .siblings1{opacity:.3}
#lrcwrap .siblings2{opacity:.1;}
#lrcwrap view{line-height:72rpx; word-break: break-all;opacity: .5;height:72rpx;overflow: hidden;transition: all ease .5s;}
#lrclist .cntloading{top:50%;position: relative;}
#lrclist:after,#lrclist:before{content: "";position: absolute;left: 0;height:50%;top:0;
 background-image: linear-gradient(rgba(0,0,0,1),rgba(0,0,0,.1)); opacity:.1;display:none;
width:100%;z-index: 3;filter: brightness(.1)}
#lrclist:after{top:auto;bottom:0;transform: rotate(180deg)}
#lrclist .cntloading{position: absolute;top: 50%;color: #fff;width: 100%; margin-top: -16rpx;}

 
.sm_title{padding:0 20rpx;font-size:24rpx;display: block;color:#6a6a6a;background-color: #eeeff0;line-height: 56rpx;}

 
.commentscount text{position: absolute;font-size: 22rpx;left: 50%;top:50%;margin-left:10rpx;margin-top: -35rpx;color: #ddd}


.flex-boxlist{overflow: hidden;display: flex;flex-wrap:wrap;padding-right: 15rpx;}
.tl_cnt{width: 33.33%;padding:10rpx 0 10rpx 15rpx;float:left;box-sizing: border-box}
.tl_cnt text{display: block;font-size: 26rpx;color: #666}
.tl_cnt .cover image{width: 100%;height: 100%;position: absolute;top:0;left:0}
.tl_cnt .cover{position: relative;height: 0;padding-top: 100%;overflow: hidden;margin-bottom: 10rpx;}
.tl_cnt .cover text{position: absolute;right: 0;bottom:0;line-height: 32rpx;color: #fff;
z-index: 2;width: 100%;padding: 8rpx;font-size: 24rpx; box-sizing: border-box;}
.tl_cnt .cover:after{height: 50%;width:100%;left: 0;top:50%;z-index: 1;content:"";position: absolute;
background-image:linear-gradient(rgba(0,0,0,0),rgba(0,0,0,.5));opacity: .5}

.tl_info{font-size: 26rpx;line-height: 1.6}
.tl_info .tli_des{color: #888;font-size: 24rpx}

.flexlist{display: flex;margin-left: 20rpx;padding: 16rpx 16rpx 16rpx 0;border-bottom:1rpx solid #e1e2e3; align-items: center;}
.flexact{flex: 0 0 100rpx; text-align: right}
.flexmain{width: 100%;padding-right: 20rpx; box-sizing:  border-box;}
.flexmain view{word-break: keep-all;overflow: hidden;
text-overflow: ellipsis; white-space: nowrap;line-height: 1.8}
.relistdes{color: #888;font-size: 24rpx;white-space: -2rpx;}

.fa_list{border-radius: 5rpx;display: inline-block}
.fa_mv image{width: 65rpx;height: 30rpx;}

.flex-image .flexlist{margin-left: 140rpx;padding: 10rpx 20rpx 10rpx 0}
.fl-image{width: 100rpx;margin-left: -120rpx;margin-right: 20rpx;position: relative}
.fl-image image{width: 100rpx;height: 100rpx; vertical-align: top;position: relative;z-index: 1}
 
.user_avator{background-size:cover;background-image: url(data:image/png;base64,)}
 
.music_cover{background-size:cover;background-image: url(data:image/png;base64,);}
