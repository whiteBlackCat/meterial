page{
     height: 100%;
 }
.page{
    font-family: 'PingFang SC', 'Helvetica Neue', Helvetica, 'Droid Sans Fallback', 'Microsoft Yahei', sans-serif;
    height: 100%;
    background: #ffffff;
}


.headName{
  margin: 48rpx 0rpx 48rpx 32rpx;
  display:flex;
  align-items:center;
  text-align: center;
}

.head{
  float: left;
  width: 130rpx;
  height: 130rpx;
}

.name{
  font-size: 35rpx;
  margin-left: 24rpx;
  float: left;
}

.grayLine{
  height: 9rpx;
  width: 100%;
  background-color: #c9d9d8;
}

.testHistory,.personalInformation{
  margin: 28rpx 0rpx 28rpx 32rpx;
  display:flex;
  align-items:center;
  text-align: center;
}



.text{
  color: #717171;
  font-size: 30rpx;
  margin-left: 16rpx;
}

.grayLine2{
  height: 1px;
  width: 100%;
  background-color: #c9d9d8;
}